    <script>
    var gauge4 = new Gauge("canvas4", {
        'mode': 'needle',
        'range': {
            'min': 0,
            'max': 90
        }
    });
    gauge4.draw(Math.floor(Math.random() * 90));
    var run = setInterval(function() {
        gauge4.draw(Math.floor(Math.random() * 90));
    }, 3500);
    </script>


    <script type="text/javascript">
    /* Javascript
     *
     * See http://jhere.net/docs.html for full documentation
     */

    $(document).ready(function() {
        var dataSet = [
            {"Date":"Sun","The first division":"Wendy","The second division":"xx","The third division":"xx"},
            {"Date":"Mon","The first division":"Liz","The second division":"xx","The third division":"xx"},
            {"Date":"Tues","The first division":"Raymond","The second division":"xx","The third division":"xx"},
            {"Date":"Wed","The first division":"Tom","The second division":"xx","The third division":"xx"},
            {"Date":"Thur","The first division":"Donald","The second division":"xx","The third division":"xx"},
            {"Date":"Fri","The first division":"Kobe","The second division":"xx","The third division":"xx"},
            {"Date":"Sat","The first division":"James","The second division":"xx","The third division":"xx"},
        ];

        $('#myTable-x').DataTable({
            "data": dataSet,
            "columns": [
                {"data": "Date", "title": "Date"},
                {"data": "The first division", "title": "The first division"},
                {"data": "The second division", "title": "The second division"},
                {"data": "The third division", "title": "The third division"},
                {
                    "data": "id",
                    "title": "operation",
                    "render": function (data, type, row) {
                        return '<a href="#update/'+row.id+'/" class="btn btn-warning btn-sm">edit</a> ' +
                            '<a href="#delete/'+row.id+'/" class="btn btn-danger btn-sm">delete</a>'
                    }
                },
            ]
        })
    });

    $(window).on('load', function() {
        $('#mapContainer').jHERE({
            center: [52.500556, 13.398889],
            type: 'smart',
            zoom: 10
        }).jHERE('marker', [52.500556, 13.338889], {
            icon: 'assets/img/marker.png',
            anchor: {
                x: 12,
                y: 32
            },
            click: function() {
                alert('Hallo from Berlin!');
            }
        })
            .jHERE('route', [52.711, 13.011], [52.514, 13.453], {
                color: '#FFA200',
                marker: {
                    fill: '#86c440',
                    text: '#'
                }
            });
    });
    </script>
    <script type="text/javascript">
    var output, started, duration, desired;

    // Constants
    duration = 5000;
    desired = '50';

    // Initial setup
    output = $('#speed');
    started = new Date().getTime();

    // Animate!
    animationTimer = setInterval(function() {
        // If the value is what we want, stop animating
        // or if the duration has been exceeded, stop animating
        if (output.text().trim() === desired || new Date().getTime() - started > duration) {
            console.log('animating');
            // Generate a random string to use for the next animation step
            output.text('' + Math.floor(Math.random() * 60)

            );

        } else {
            console.log('animating');
            // Generate a random string to use for the next animation step
            output.text('' + Math.floor(Math.random() * 120)

            );
        }
    }, 5000);
    </script>
    <script type="text/javascript">
    $('#getting-started').countdown('2015/01/01', function(event) {
        $(this).html(event.strftime(

            '<span>%M</span>' + '<span class="start-min">:</span>' + '<span class="start-min">%S</span>'));
    });

    </script>

